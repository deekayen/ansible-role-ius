[ius-dev]
name=IUS Community Packages for Enterprise Linux {{ ansible_distribution_major_version }} - $basearch - Dev
{% if ius_enable_mirrors %}
mirrorlist=https://mirrors.iuscommunity.org/mirrorlist?repo={{ ius_repo_mirror_name}}{{ ansible_distribution_major_version }}-dev&arch=$basearch&protocol=http
{% else %}
baseurl={{ ius_repo_url }}/development/{{ ansible_distribution }}/{{ ansible_distribution_major_version }}/$basearch
{% endif %}
failovermethod=priority
enabled={{ ius_enable_dev }}
gpgcheck=1
gpgkey={{ ius_repo_url }}/IUS-COMMUNITY-GPG-KEY
{% if proxy_url %}
proxy={{ proxy_url }}
{% endif %}

